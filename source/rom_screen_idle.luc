module rom_screen_idle (
    output out[11][5]
  ) {

  always {
    out[0] = b11111;
    out[1] = b10001;
    out[2] = b10001;
    out[3] = b10001;
    out[4] = b11111;
    out[5] = b00000;
    out[6] = b11111;
    out[7] = b10000;
    out[8] = b11111;
    out[9] = b00001;
    out[10] = b11111;
  }
}
